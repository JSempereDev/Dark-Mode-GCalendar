{
  "manifest_version": 3,
  "name": "Dark GCalendar",
  "version": "1.0",
  "description": "Extension to add a dark mode to Google Calendar",
  "permissions": [
    "tabs",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  },
  "action": {
    "default_popup": "./code/popup.html"
  }
}