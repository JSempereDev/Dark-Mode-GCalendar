{
  "name": "Dark Gcalendar",
  "description": "Inject JS to change the Google calendar",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "./code/main.js"
      ]
    }
  ],
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  }
}


